<decoder-config xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder.xsd" showEmptyPanes="no" >
  <version author="Bob Jacobsen" version="1" lastUpdated="20150812"/>
  <!-- version 1 - test only -->
  
  <decoder>
    <family name="LNSV2-using boards" mfg="Public-domain and DIY">
      <model model="Standard-Stummidecoder"/>
    </family>
    <programming direct="no" paged="no" register="no" ops="no">
        <mode>LOCONETSV2MODE</mode>
    </programming>
    <variables>

     <!-- Is the following actually accessible via the SV protocol? -->
     <!-- This sets a single JMRI-style composite address in two CVs  -->
      <variable CV="3" comment="Board SV address" item="Long Address" default="255">
        <splitVal highCV="4" upperMask="XXXXXVVV"/>
        <label>Lange Adresse</label>
        <tooltip>Lange 14-Bit Adresse in SV 3+4</tooltip>
      </variable>

     <variable CV="2" item="Decoder Version" >
        <decVal/>
        <label>Manufacturer Version No.</label>
      </variable>

      <variable CV="8" readOnly="yes" item="Manufacturer">
        <decVal/>
        <label>Manufacturer ID: </label>
      </variable>

      <variable item="Betriebsparameter1" CV="47">
        <hexVal/>
        <label>Betriebsart: </label>
      </variable>
      <variable item="Betriebsparameter2" CV="48">
        <splitVal highCV="49" />
        <label>LocoNet ID: </label>
        <tooltip>Änderung gilt ab nächstem Reset</tooltip>
      </variable>
      
      <!-- Parameter für Funktion 1 -->
      <variable item="Mode1Bit0" CV="50" mask="XXXXXXXV">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Blinken: </label>
      </variable>
      <variable item="Mode1Bit1" CV="50" mask="XXXXXXVX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>StartMode: </label>
      </variable>
      <variable item="Mode1Bit2" CV="50" mask="XXXXXVXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>SoftLed: </label>
      </variable>
      <variable item="Mode1" CV="50" >
        <hexVal/> 
        <label>Modebyte(hex): </label>
      </variable> 
      <variable item="Parameter1 Byte2" CV="51">
        <decVal/>
        <label>EIN(10ms): </label>
      </variable>
      <variable item="Parameter1 Byte3" CV="52">
        <decVal/>
        <label>AUS(10ms): </label>
      </variable>
      <variable item="Parameter1 Byte4" CV="53">
        <decVal/>
        <label>Erstes EIN(10ms): </label>
      </variable>
      <variable item="Parameter1 Byte5" CV="54">
        <decVal/>
        <label>Status: </label>
      </variable>

      <!-- Parameter für Funktion 2 -->
      <variable item="Mode2Bit0" CV="55" mask="XXXXXXXV">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>AutoOFF: </label>
      </variable>
      <variable item="Mode2Bit1" CV="55" mask="XXXXXXVX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Direct-Mode: </label>
      </variable>
      <variable item="Mode2Bit2" CV="55" mask="XXXXXVXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Kein PosCheck: </label>
      </variable>
      <variable item="Mode2" CV="55" >
        <hexVal/> 
        <label>Modebyte(hex): </label>
      </variable> 
      <variable item="Parameter2 Byte2" CV="56">
        <decVal/>
        <label>Position '0': </label>
      </variable>
      <variable item="Parameter2 Byte3" CV="57">
        <decVal/>
        <label>Position '1': </label>
      </variable>
      <variable item="Parameter2 Byte4" CV="58">
        <decVal/>
        <label>Geschwindigkeit: </label>
      </variable>
      <variable item="Parameter2 Byte5" CV="59">
        <decVal/>
        <label>Status: </label>
      </variable>

      <!-- Parameter für Funktion 3 -->
      <variable item="Mode3Bit0" CV="60" mask="XXXXXXXV">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Out1-Hart: </label>
      </variable>
      <variable item="Mode3Bit1" CV="60" mask="XXXXXXVX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Out2-Hart: </label>
      </variable>
      <variable item="Mode3Bit2" CV="60" mask="XXXXXVXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Out3-Hart: </label>
      </variable>
      <variable item="Mode3Bit7" CV="60" mask="VXXXXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Invertieren: </label>
      </variable>
      <variable item="Mode3" CV="60" >
        <hexVal/> 
        <label>Modebyte(hex): </label>
      </variable> 
      <variable item="Parameter3 Byte2" CV="61">
        <hexVal/>
        <label>Bitmuster0(hex): </label>
      </variable>
      <variable item="Parameter3 Byte3" CV="62">
        <hexVal/>
        <label>Bitmuster2(hex): </label>
      </variable>
      <variable item="Parameter3 Byte4" CV="63">
        <decVal/>
        <label>VorsigIndex: </label>
      </variable>
      <variable item="Parameter3 Byte5" CV="64">
        <hexVal/>
        <label>DunkelStates(hex): </label>
      </variable>

      <!-- Parameter für Funktion 4 -->
      <variable item="Mode4Bit0" CV="65" mask="XXXXXXXV">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit0: </label>
      </variable>
      <variable item="Mode4Bit1" CV="65" mask="XXXXXXVX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit1: </label>
      </variable>
      <variable item="Mode4Bit2" CV="65" mask="XXXXXVXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit2: </label>
      </variable>
      <variable item="Mode4Bit3" CV="65" mask="XXXXVXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit3: </label>
      </variable>
      <variable item="Mode4Bit4" CV="65" mask="XXXVXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit4: </label>
      </variable>
      <variable item="Mode4Bit5" CV="65" mask="XXVXXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit5: </label>
      </variable>
      <variable item="Mode4Bit6" CV="65" mask="XVXXXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit6: </label>
      </variable>
      <variable item="Mode4Bit7" CV="65" mask="VXXXXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit7: </label>
      </variable>
      <variable item="Mode4" CV="65" >
        <hexVal/> 
        <label>Modebyte(hex): </label>
      </variable> 
      <variable item="Parameter4 Byte2" CV="66">
        <decVal/>
        <label>Param1: </label>
      </variable>
      <variable item="Parameter4 Byte3" CV="67">
        <decVal/>
        <label>Param2: </label>
      </variable>
      <variable item="Parameter4 Byte4" CV="68">
        <decVal/>
        <label>Param3: </label>
      </variable>
      <variable item="Parameter4 Byte5" CV="69">
        <decVal/>
        <label>Status: </label>
      </variable>

      <!-- Parameter für Funktion 5 -->
      <variable item="Mode5Bit0" CV="70" mask="XXXXXXXV">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit0: </label>
      </variable>
      <variable item="Mode5Bit1" CV="70" mask="XXXXXXVX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit1: </label>
      </variable>
      <variable item="Mode5Bit2" CV="70" mask="XXXXXVXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit2: </label>
      </variable>
      <variable item="Mode5Bit3" CV="70" mask="XXXXVXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit3: </label>
      </variable>
      <variable item="Mode5Bit4" CV="70" mask="XXXVXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit4: </label>
      </variable>
      <variable item="Mode5Bit5" CV="70" mask="XXVXXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit5: </label>
      </variable>
      <variable item="Mode5Bit6" CV="70" mask="XVXXXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit6: </label>
      </variable>
      <variable item="Mode5Bit7" CV="70" mask="VXXXXXXX">
        <enumVal> 
          <enumChoice> <choice>off</choice> </enumChoice> <enumChoice> <choice>on</choice>  </enumChoice>
        </enumVal>
        <label>Bit7: </label>
      </variable>
      <variable item="Mode5" CV="70" >
        <hexVal/> 
        <label>Modebyte(hex): </label>
      </variable> 
      <variable item="Parameter5 Byte2" CV="71">
        <decVal/>
        <label>Param1: </label>
      </variable>
      <variable item="Parameter5 Byte3" CV="72">
        <decVal/>
        <label>Param2: </label>
      </variable>
      <variable item="Parameter5 Byte4" CV="73">
        <decVal/>
        <label>Param3: </label>
      </variable>
      <variable item="Parameter5 Byte5" CV="74">
        <decVal/>
        <label>Status: </label>
      </variable>


      <!-- Parameter für Funktion 3 -->
      <variable item="Dummy 75" CV="75">
        <decVal/>
      </variable>
      <variable item="Dummy 76" CV="76">
        <decVal/>
      </variable>
      <variable item="Dummy 77" CV="77">
        <decVal/>
      </variable>
      <variable item="Dummy 78" CV="78">
        <decVal/>
      </variable>
      <variable item="Dummy 79" CV="79">
        <decVal/>
