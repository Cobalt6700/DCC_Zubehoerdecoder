    </variables>
  </decoder>
<pane xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/programmer.xsd">
  <name>Parameter</name>
  <row>
  <column>
    <label>
      <text>allg. Betriebsparameter    </text>
    </label>
    <display item="Betriebsparameter1" format="checkbox"/>
    <display item="Betriebsparameter2" format="checkbox"/>
    <display item="Betriebsparameter3" format="checkbox"/>
  </column>
  <separator/>
  <column>
    <label>
      <text> STATIC  </text>
    </label>
      <separator/>
    <display item="Mode1"     format="checkbox"/>
    <display item="Mode1Bit0" format="checkbox"/>
    <display item="Mode1Bit1" format="checkbox"/>
    <display item="Mode1Bit2" format="checkbox"/>
    <display item="Mode1Bit3" format="checkbox"/>
    <display item="Mode1Bit4" format="checkbox"/>
    <display item="Mode1Bit5" format="checkbox"/>
    <display item="Mode1Bit6" format="checkbox"/>
    <display item="Mode1Bit7" format="checkbox"/>
      <separator/>

    <display item="Parameter1 Byte2" format="checkbox"/>
    <display item="Parameter1 Byte3" format="checkbox"/>
    <display item="Parameter1 Byte4" format="checkbox"/>
    <display item="Parameter1 Byte5" format="checkbox"/>
  </column>
  <separator/>
  <separator/>
  <column>
